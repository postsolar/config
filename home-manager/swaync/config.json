{
  "$schema": "https://github.com/ErikReider/SwayNotificationCenter/raw/refs/heads/main/src/configSchema.json",
  "cssPriority": "user",
  "notification-inline-replies": true,
  "notification-icon-size": 32,
  "notification-window-width": 400,
  "notification-visibility": {},
  "widgets": [
    "mpris",
    "volume",
    "inhibitors",
    "title",
    "dnd",
    "notifications"
  ],
  "widget-config": {
    "volume": {
      "show-per-app": true,
      "show-per-app-label": true
    },
    "inhibitors": {
      "button-text": "clear all"
    },
    "title": {
      "button-text": "clear all"
    },
    "dnd": {
      "text": "do not disturb"
    }
  }
}

