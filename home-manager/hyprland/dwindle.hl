general {
  layout = dwindle
  resize_on_border = yes
  extend_border_grab_area = 30
}

dwindle {
  preserve_split = yes
}

cursor {
  no_warps = yes
}

gestures {
  workspace_swipe = yes
  workspace_swipe_forever = yes  
  workspace_swipe_create_new = no
}

source = $conf/binds/core.hl
source = $conf/binds/dwindle.hl

